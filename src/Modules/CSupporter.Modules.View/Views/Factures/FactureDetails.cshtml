@model CSupporter.Modules.View.Models.EntireFactureDto
@{
    ViewData["Title"] = "FactureDetails";
}

<form asp-action="FactureEdit">
    <input asp-for="FactureId" hidden />
    <br />
    <div class="container border p-3">
        <h1 class="text-primary">Edit Facture</h1>

        <hr />
        <div class="row">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="col-2">
                <label asp-for="FactureNo" class="control-label pt-2" style="font-size:20px;"></label>
                <div>@Model.FactureNo</div>>
            </div>
            <div class="col-2">
                <label asp-for="ContractorLastName" class="control-label pt-2" style="font-size:20px;"></label>
                <div>@Model.ContractorLastName</div>>
            </div>
            <div class="col-2">
                <label asp-for="FactureType" class="control-label pt-2" style="font-size:20px;"></label>
                <div>@Model.FactureType</div>>
            </div>

            <div class="col-5 offset-2">
                <a asp-action="FacturesIndex" class="btn-primary btn form-control ">Back to List</a>
            </div>

        </div>

        <div>
        </div>
    </div>
</form>